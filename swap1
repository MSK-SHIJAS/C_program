#include <stdio.h>

int main() {
    int a, b, temp;

    printf("Enter numbers: ");
    scanf("%d%d",&a,&b);
    printf("before swapping a = %d", a);
    printf(" b = %d\n", b);
    temp = a;
    a = b;
    b = temp;
    printf("After swapping a = %d", a);
    printf(" b = %d", b);

    return 0;
}
