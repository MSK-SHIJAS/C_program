#include<stdio.h>
int main()
{
    char c;
    printf("Enter a character: ");
    scanf("%c", &c);

    if (c == 'A' || c == 'a' || c == 'E' || c == 'e' || c == 'I' || c == 'i' || c == 'O' || c == 'o' || c == 'U' || c == 'u')
        printf("\nThe entered character is a VOWEL.");
    else
        printf("\nThe entered character is a CONSONANT.");

    return 0;
}
